[DIM_JOGADOR]:
SELECT "PK_PLAYER",
	"NOME_COMPLETO",
	"NOME",
	"SOBRENOME",
	"EM_ATIVIDADE",
	"DATA_NASCIMENTO",
	"UNIVERSIDADE",
	"PAIS",
	"ALTURA_POLEGADAS",
	"ALTURA_CM",
	"ALTURA_PES",
	"PESO_LIBRAS",
	"PESO_KG",
	"TEMPORADAS",
	"NUM_CAMISA",
	"POSICAO",
	"PRIMEIRO_ANO_NBA",
	"ULTIMO_ANO_NBA",
	"RODADA_DRAFT",
	"NUMERO_ESCOLHA_DRAFT",
	"TOP_75_JOGADORES",
	"TIME_DRAFT",
	"TIME_DRAFT_CIDADE",
	"TIME_ABRV",
	"ORGANIZACAO_ANTES_NBA",
	"ORGANIZACAO_TIPO"
FROM "NBA_CONSUMPTION_DB"."NBA_CONSUMPTION_SCHEMA"."DIM_JOGADOR";
Store [DIM_JOGADOR] into [lib://DataFiles/Extraidos/NBA/DIM_JOGADOR.qvd] (qvd);
Drop Table [DIM_JOGADOR];

